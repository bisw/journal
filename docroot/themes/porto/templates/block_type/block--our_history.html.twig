{%
set classes = [
'block',
'block-' ~ configuration.provider|clean_class,
'block-' ~ plugin_id|clean_class,
]

%}
<div{{ attributes.addClass(classes) }}>
    {{ title_prefix }}
<div class="container">
<div class="row">
    <div class="col-md-12">
        <h3 class="heading-primary mt-xl">Our <strong>History</strong></h3>
    </div>
</div>

<div class="row">
    <div class="col-md-12">
        <ul class="history">
            {{ content.field_our_history }}
        </ul>

    </div>
</div>

</div>
    {{ title_suffix }}
</div>